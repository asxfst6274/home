                  </div>
                </div>
              </div>

              <!--Tab-->
              <div class="tab-content-box-item" id="gold">
                <div class="banking-tab-content-item">
                  <div class="row">
                    <div class="col-xl-6">
                      <div class="banking-tab-img-box">
                        <div
                          class="banking-tab-img-box__bg"
                          style="
                            background-image: url(/home/images/resources/banking-tab-3.jpg);
                          "
                        ></div>
                      </div>
                    </div>
                    <div class="col-xl-6">
                      <div class="banking-tab-text-box">
                        <div class="inner-title">
