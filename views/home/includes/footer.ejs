<div
  class="d-flex gap-2 position-absolute shadow"
  style="top: 5rem; z-index: 999; right: 0.5rem"
>
  <div id="google_translate_element"></div>
</div>

<footer class="site-footer">
  <div class="site-footer__shape-1 float-bob-x">
    <img src="/home/assets/images/shapes/site-footer-shape-1.png" alt="" />
  </div>
  <div
    class="site-footer__bg"
    style="
      background-image: url(/home/assets/images/backgrounds/site-footer-bg.png);
    "
  ></div>
  <div class="site-footer__top">
    <div class="container">
      <div class="row">
        <div
          class="col-xl-3 col-lg-6 col-md-6 wow fadeInUp"
          data-wow-delay="100ms"
        >
          <div class="footer-widget__column footer-widget__about">
            <div class="footer-widget__logo">
              <a href="/"><img src="<%= dataFile.logo2 %>" alt="" /></a>
            </div>
            <p class="footer-widget__about-text">
              Your Trusted Partner in Banking Excellence, Delivering Seamless
              Financial Solutions for a Brighter Future.
            </p>
            <div class="site-footer__social">
              <a href="javascript:(0)"><i class="fab fa-twitter"></i></a>
              <a href="javascript:(0)"><i class="fab fa-facebook"></i></a>
              <a href="javascript:(0)"><i class="fab fa-pinterest-p"></i></a>
              <a href="javascript:(0)"><i class="fab fa-instagram"></i></a>
            </div>
          </div>
        </div>
        <div
          class="col-xl-3 col-lg-6 col-md-6 wow fadeInUp"
          data-wow-delay="200ms"
        >
          <div class="footer-widget__column footer-widget__link">
            <div class="footer-widget__title-box">
              <h3 class="footer-widget__title">Explore</h3>
            </div>
            <ul class="footer-widget__link-list list-unstyled">
              <li><a href="/accounts">Accounts</a></li>
              <li><a href="/loan">Loan</a></li>
              <li><a href="/about">About</a></li>
              <li><a href="/team">Our Team</a></li>
              <li><a href="/contact">Contact</a></li>
            </ul>
          </div>
        </div>
        <div
          class="col-xl-3 col-lg-6 col-md-6 wow fadeInUp"
          data-wow-delay="300ms"
        >
          <div class="footer-widget__column footer-widget__newsletter">
            <div class="footer-widget__title-box">
              <h3 class="footer-widget__title">Newsletter</h3>
            </div>
            <form
              class="footer-widget__newsletter-form mc-form"
              data-url="MC_FORM_URL"
              novalidate="novalidate"
            >
              <div class="footer-widget__newsletter-form-input-box">
                <input type="email" placeholder="Your email" name="EMAIL" />
                <button type="submit" class="footer-widget__newsletter-btn">
                  <span class="fas fa-paper-plane"></span>
                </button>
              </div>
            </form>
            <div class="mc-form__response"></div>
            <ul class="footer-widget__Contact-list list-unstyled">
              <li>
                <div class="icon">
                  <span class="fas fa-envelope"></span>
                </div>
                <div class="text">
                  <a href="mailto:<%= dataFile.email %>"
                    ><%= dataFile.email %></a
                  >
                </div>
              </li>
              <% if(dataFile.phone){ %>
              <li>
                <div class="icon">
                  <span class="fas fa-phone-square"></span>
                </div>
                <div class="text">
                  <a href="tel:<%= dataFile.phone  %>"><%= dataFile.phone %></a>
                </div>
              </li>
              <% } %>
            </ul>
          </div>
        </div>
        <div
          class="col-xl-3 col-lg-6 col-md-6 wow fadeInUp"
          data-wow-delay="500ms"
        >
          <div class="footer-widget__column footer-widget__portfolio">
            <div class="footer-widget__title-box">
              <h3 class="footer-widget__title">Gallery</h3>
            </div>
            <ul class="footer-widget__portfolio-list list-unstyled clearfix">
              <li>
                <div class="footer-widget__portfolio-img">
                  <img
                    src="/home/assets/images/project/footer-widget-portfolio-img-1.jpg"
                    alt=""
                  />
                  <a href="javascript:(0)"
                    ><span class="fab fa-instagram"></span
                  ></a>
                </div>
              </li>
              <li>
                <div class="footer-widget__portfolio-img">
                  <img
                    src="/home/assets/images/project/footer-widget-portfolio-img-2.jpg"
                    alt=""
                  />
                  <a href="javascript:(0)"
                    ><span class="fab fa-instagram"></span
                  ></a>
                </div>
              </li>
              <li>
                <div class="footer-widget__portfolio-img">
                  <img
                    src="/home/assets/images/project/footer-widget-portfolio-img-3.jpg"
                    alt=""
                  />
                  <a href="javascript:(0)"
                    ><span class="fab fa-instagram"></span
                  ></a>
                </div>
              </li>
              <li>
                <div class="footer-widget__portfolio-img">
                  <img
                    src="/home/assets/images/project/footer-widget-portfolio-img-4.jpg"
                    alt=""
                  />
                  <a href="javascript:(0)"
                    ><span class="fab fa-instagram"></span
                  ></a>
                </div>
              </li>
              <li>
                <div class="footer-widget__portfolio-img">
                  <img
                    src="/home/assets/images/project/footer-widget-portfolio-img-5.jpg"
                    alt=""
                  />
                  <a href="javascript:(0)"
                    ><span class="fab fa-instagram"></span
                  ></a>
                </div>
              </li>
              <li>
                <div class="footer-widget__portfolio-img">
                  <img
                    src="/home/assets/images/project/footer-widget-portfolio-img-6.jpg"
                    alt=""
                  />
                  <a href="javascript:(0)"
                    ><span class="fab fa-instagram"></span
                  ></a>
                </div>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="site-footer__bottom">
    <div class="container">
      <div class="row">
        <div class="col-xl-12">
          <div class="site-footer__bottom-inner">
            <p class="site-footer__bottom-text">
              Â© Copyright
              <script>
                document.writeln(new Date().getFullYear());
              </script>
              by <a href="javascript:(0)"><%= dataFile.name %></a>
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</footer>
