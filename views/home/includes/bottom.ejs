<div class="mobile-nav__wrapper">
  <div class="mobile-nav__overlay mobile-nav__toggler"></div>
  <!-- /.mobile-nav__overlay -->
  <div class="mobile-nav__content">
    <span class="mobile-nav__close mobile-nav__toggler"
      ><i class="fa fa-times"></i
    ></span>

    <div class="logo-box">
      <a href="/" aria-label="logo image"
        ><img src="<%= dataFile.logo2 %>" width="135" alt=""
      /></a>
    </div>
    <!-- /.logo-box -->
    <div class="mobile-nav__container"></div>
    <!-- /.mobile-nav__container -->

    <ul class="mobile-nav__contact list-unstyled">
      <li>
        <i class="fa fa-envelope"></i>
        <a href="mailto:<%= dataFile.email %>"><%= dataFile.email %></a>
      </li>
      <% if(dataFile.phone){ %>
      <li>
        <i class="fa fa-phone-alt"></i>
        <a href="tel:<%= dataFile.phone  %>"><%= dataFile.phone %></a>
      </li>
      <% } %>
    </ul>
    <!-- /.mobile-nav__contact -->
    <div class="mobile-nav__top">
      <div class="mobile-nav__social">
        <a href="javascript:(0)" class="fab fa-twitter"></a>
        <a href="javascript:(0)" class="fab fa-facebook-square"></a>
        <a href="javascript:(0)" class="fab fa-pinterest-p"></a>
        <a href="javascript:(0)" class="fab fa-instagram"></a>
      </div>
      <!-- /.mobile-nav__social -->
    </div>
    <!-- /.mobile-nav__top -->
  </div>
  <!-- /.mobile-nav__content -->
</div>
<!-- /.mobile-nav__wrapper -->

<div class="search-popup">
  <div class="search-popup__overlay search-toggler"></div>
  <!-- /.search-popup__overlay -->
  <div class="search-popup__content">
    <form action="#">
      <label for="search" class="sr-only">search here</label
      ><!-- /.sr-only -->
      <input type="text" id="search" placeholder="Search Here..." />
      <button type="submit" aria-label="search submit" class="thm-btn">
        <i class="icon-magnifying-glass"></i>
      </button>
    </form>
  </div>
  <!-- /.search-popup__content -->
</div>
<!-- /.search-popup -->

<a
  href="javascript:(0)"
  data-target="html"
  class="scroll-to-target scroll-to-top"
  ><i class="icon-right-arrow"></i
></a>

